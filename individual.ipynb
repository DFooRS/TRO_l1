   "source": [
    "С какой силой давит человек массой 70кг на пол лифта , движущегося с ускорением 1м/с^2 вертикально вверх?\n",
    "\n",
    "$ P = F = m * (g + a) $"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "f522fb86",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Сила давления P равна: 616.0 Н\n"
     ]
    }
   ],
   "source": [
    "m = 70\n",
    "a = 1\n",
    "g = 9.8\n",
    "N = m * (g - a)\n",
    "P = N\n",
    "print(f\"Сила давления P равна: {P} Н\")"
   ]
  }
